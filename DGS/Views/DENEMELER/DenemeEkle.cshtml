@model DGS.Models.DENEMELER

@{
    ViewBag.Title = "DenemeEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="card w-75">
                    <div class="card-body">
                        <h3 class="card-title">Deneme Ekleme Sayfası</h3>
                        @using (Html.BeginForm("DenemeEkle", "DENEMELER", FormMethod.Post))
                        {
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="aciklama">Deneme Adı</label>
                                        @Html.TextBoxFor(Model => Model.DENEMEADI, new { @class = "form-control", @placeholder ="Deneme Adı Girin..." })
                                    </div>
                                    <div class="form-group">
                                        <input type="submit" value="Kaydet" />
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>